}

.wrapper {
  padding: 20px 0;
}

.wrapper h3 {
  font-size: 2em;
  text-align: center;
  font-family: 'Quattrocento Sans', sans-serif;
}
.wrapper h4 {
  font-size: 1.2em;
  padding: 0;
  font-weight: bold;
}
.wrapper p {
  margin: 20px auto;
}
.wrapper img {
  margin: 10px auto;
}

.wrapper div {
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  -o-box-sizing: border-box;
  -ms-box-sizing: border-box;
  box-sizing: border-box;
  padding: 1%;
  position: relative;
  height: 500px;
}










TABLE
{
    BORDER-TOP-STYLE: black 1px solid;
    BORDER-RIGHT-STYLE: black 1px solid;
    BORDER-LEFT-STYLE: black 1px solid;
    BORDER-BOTTOM-STYLE: black 1px solid;
    padding:15px;
    font-size: 12px;
    WIDTH: 100%;
}
TABLE.result
{
    BORDER-RIGHT: black 1px solid;
    BORDER-TOP: black 1px solid;
    BORDER-LEFT: black 1px solid;
    BORDER-BOTTOM: black 1px solid;
    BORDER-COLLAPSE: collapse
}

TABLE.selPayWay th
{
    BORDER-RIGHT: black 1px solid;
    BORDER-TOP: black 1px solid;
    BORDER-LEFT: black 1px solid;
    BORDER-BOTTOM: black 1px solid;
    TEXT-ALIGN: center;
    background-color: silver;
    white-space: nowrap;
}
TABLE.selPayWay td
{
    BORDER-RIGHT: black 1px solid;
    BORDER-TOP: black 1px solid;
    BORDER-LEFT: black 1px solid;
    BORDER-BOTTOM: black 1px solid;
    TEXT-ALIGN: left;
}
TABLE.selPayWay
{
    BORDER-COLLAPSE: collapse;
    WIDTH: 80%;
}
table.login
{
    
    PADDING-RIGHT: 15px;
    PADDING-LEFT: 15px;
    WIDTH: 50%;
    PADDING-TOP: 15px;
    TEXT-ALIGN:center ;
}
.resultHead
{
    font-weight: bolder;
    background-color: #ffffcc;
    text-align: right;
    //border-bottom: black thin solid;
}

TH.result
{
    BORDER-RIGHT: black 1px solid;
    BORDER-TOP: black 1px solid;
    BORDER-LEFT: black 1px solid;
    BORDER-BOTTOM: black 1px solid;
    BACKGROUND-COLOR: lightyellow
}
.list tr td
{
    border-right: black 1px solid;
    border-top: black 1px solid;
    border-left: black 1px solid;
    border-bottom: black 1px solid;
    TEXT-ALIGN: left;
    BORDER: black 1px solid;
}

.list tr th
{
    border-right: black 1px solid;
    border-top: black 1px solid;
    border-left: black 1px solid;
    border-bottom: black 1px solid;
    TEXT-ALIGN: left;
    BORDER: black 1px solid;
    
}

TD
{
    TEXT-ALIGN: left;

}

TD.result
{
    BORDER-RIGHT: black 1px solid;
    PADDING-RIGHT: 10px;
    BORDER-TOP: black 1px solid;
    PADDING-LEFT: 10px;
    BORDER-LEFT: black 1px solid;
    BORDER-BOTTOM: black 1px solid
}

TD.resultTotal
{
    border-right: black 1px solid;
    padding-right: 10px;
    border-top: black 1px solid;
    padding-left: 10px;
    border-left: black 1px solid;
    border-bottom: black 1px solid;
    font-weight: bold;
    text-align: right;
}

TD.resultCur
{
    BORDER-RIGHT: black 1px solid;
    PADDING-RIGHT: 10px;
    BORDER-TOP: black 1px solid;
    PADDING-LEFT: 10px;
    BORDER-LEFT: black 1px solid;
    BORDER-BOTTOM: black 1px solid;
    TEXT-ALIGN: right;
}

